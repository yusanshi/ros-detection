<!-- Remember to: ROS_NAMESPACE=/usb_camX python3.6 server.py -->
<launch>
  <arg name="image_src_topic" default="/image_raw"/>
  <node name="zmq_image_detection" pkg="zmq_image_detection" type="zmq_image_detection_handler" required="true" args="$(arg image_src_topic)" ns="$(env ROS_NAMESPACE)"/>
  <node name="detected_objects_visualizer" pkg="detected_objects_visualizer" type="visualize_rects" required="true" args="$(arg image_src_topic)" ns="$(env ROS_NAMESPACE)"/>
</launch>